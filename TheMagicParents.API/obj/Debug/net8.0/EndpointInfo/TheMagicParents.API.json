                "type": "object",
                "properties": {
                  "Location": {
                    "type": "string"
                  },
                  "UserName": {
                    "type": "string"
                  },
                  "PhoneNumber": {
                    "maxLength": 11,
                    "minLength": 11,
                    "type": "string"
                  },
