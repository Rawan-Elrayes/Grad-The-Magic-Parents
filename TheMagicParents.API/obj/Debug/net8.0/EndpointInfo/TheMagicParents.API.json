                    "type": "string",
                    "format": "email"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "PersonalPhoto": {
                    "type": "string",
                    "format": "binary"
                  },
                  "IdCardFrontPhoto": {
                    "type": "string",
                    "format": "binary"
                  },
                  "IdCardBackPhoto": {
                    "type": "string",
                    "format": "binary"
                  },
                  "CityId": {
                    "type": "integer",
                    "format": "int32"
                  }
                }
              },
              "encoding": {
                "Location": {
                  "style": "form"
                },
                "UserName": {
                  "style": "form"
                },
                "PhoneNumber": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "PersonalPhoto": {
                  "style": "form"
                },
                "IdCardFrontPhoto": {
                  "style": "form"
                },
                "IdCardBackPhoto": {
                  "style": "form"
                },
                "CityId": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
